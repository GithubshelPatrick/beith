<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".recemChegadoActivity">

    <TextView
            android:text="Recem Chegado"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView130"
            android:textColor="#161313" android:textAlignment="center"
            android:textSize="24sp"
            android:layout_marginTop="16dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
    />
    <TextView
            android:text="Assuntos :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView131"
            android:textColor="#161313"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="23dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/BtnTeudadZeut"/>
    <Button
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnSeguroSaude"
            android:text="Seguro de saúde e inscrição no Kupot Cholim"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnUlpanzinho" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>
    <Button
            android:text="Cédula de identidade, Teudad Zeut"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnTeudadZeut"
            android:layout_marginTop="123dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnCertificadoDeOle"
            android:text="Certificado de olê"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnTeudadZeut" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Abertura de conta bancária "
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnAberturaDeConta"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnCertificadoDeOle" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Ajuda financeira – bolsa de absorção"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnAjudaFinaceira"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnAberturaDeConta" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Ulpan de estudos de hebraico"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnUlpanzinho"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnAjudaFinaceira" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>
    <Button
            android:text="Inscrição dos filhos no sistema educacional"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnIscricaoDeFilhos"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnSeguroSaude" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>
    <Button
            android:text="Garantia de renda"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/button29"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnIscricaoDeFilhos" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>
    <Button
            android:text="Facilidades na Alfândega e no IVA"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button30" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button29"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.031"/>
    <Button
            android:text="Voltar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/BtnVoltarRecemChegado" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button30"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>