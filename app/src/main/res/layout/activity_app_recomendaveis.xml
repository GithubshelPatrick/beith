            android:layout_height="70dp"
            android:id="@+id/textView9" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView14" android:layout_marginStart="8dp"
