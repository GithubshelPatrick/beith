            android:layout_width="70dp"
            android:layout_height="70dp" app:srcCompat="@drawable/pango"
            android:id="@+id/imageView80" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltardeApps"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/imageView79"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"/>
