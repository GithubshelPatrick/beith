            android:layout_height="70dp"
            android:id="@+id/textView23" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView15" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltardeApps"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView9"
