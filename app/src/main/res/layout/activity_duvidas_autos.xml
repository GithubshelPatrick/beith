<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DuvidasAutosActivity">

    <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp" app:srcCompat="@drawable/duvida"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:id="@+id/imageView83" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp" app:srcCompat="@drawable/automovel"
            android:id="@+id/imageView88"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView83" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="Dúvidas sobre autos ou habilitaçao"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView162" app:layout_constraintEnd_toStartOf="@+id/imageView88"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/imageView83"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.431" app:layout_constraintVertical_bias="0.039"
            android:textAlignment="center" android:textColor="#161313"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/carteirademotorista"
            android:id="@+id/imageView89"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView162"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.104"/>
    <Button
            android:text="HABILITAÇAO"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnHabilitacaoo" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView89"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView162"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.105"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/multa"
            android:id="@+id/imageView93" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnHabilitacaoo"
            app:layout_constraintHorizontal_bias="0.976" app:layout_constraintVertical_bias="0.021"/>
    <Button
            android:text="multas e pontuçao"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button5" app:layout_constraintEnd_toStartOf="@+id/imageView93"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnHabilitacaoo" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.02"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/batida"
            android:id="@+id/imageView94" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button5"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.024"/>
    <Button
            android:text="Tipos de seguro"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button7" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView94"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button5"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.024"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/testecarro"
            android:id="@+id/imageView98"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button7"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.069"/>
    <Button
            android:text="teste anual"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button14" app:layout_constraintEnd_toStartOf="@+id/imageView98"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button7" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.069"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/transferi"
            android:id="@+id/imageView99"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button14"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.083"/>
    <Button
            android:text="Transferencia do bem"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button15" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView99"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button14"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.086"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/finance"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView102" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button15"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.14"/>
    <Button
            android:text="tipos de financiamentos"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button16" app:layout_constraintEnd_toStartOf="@+id/imageView102"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button15" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.139"/>
    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            android:id="@+id/BtnVoltardeDuvAutos" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button16"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="1.0"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="120dp" app:srcCompat="@drawable/maladois"
            android:id="@+id/imageView103"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltardeDuvAutos"
            app:layout_constraintTop_toBottomOf="@+id/button16"/>
</androidx.constraintlayout.widget.ConstraintLayout>