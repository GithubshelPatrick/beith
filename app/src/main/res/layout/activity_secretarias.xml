<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SecretariasActivity">

    <TextView
            android:text="Secretarias"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView90" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:textAlignment="center" android:textColor="#161313"
            android:textSize="30sp" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/BtnVoltarDeSecretaria" android:layout_marginEnd="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView90" app:layout_constraintHorizontal_bias="0.511"
            app:layout_constraintVertical_bias="0.987" android:layout_marginStart="8dp"/>
    <Button
            android:text="Misrad ha Rishui / transportes"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/BtnHaRichui" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeSecretaria"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView90"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.07"/>
    <Button
            android:text="Misrad ha Klita / Absorção"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/button10" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeSecretaria"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnHaRichui"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.061"/>
    <Button
            android:text="Misrad ha Pniim / "
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button11" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeSecretaria"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button10"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.082"/>
    <Button
            android:text="Misrad ha Briut / Saúde"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/button12" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeSecretaria"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button11"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.107"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/beithbrasil"
            android:id="@+id/imageView57"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button12" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeSecretaria"/>
</androidx.constraintlayout.widget.ConstraintLayout>