<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BeneficiosActivity">

    <TextView
            android:text="Beneficios"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView120" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:textColor="#161313"
            android:textAlignment="center" android:textSize="24sp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="25dp" android:layout_marginBottom="205dp"
            app:layout_constraintBottom_toTopOf="@+id/textView124"/>
    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="8dp" android:id="@+id/BtnVoltarDeBeneficios" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView120" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintVertical_bias="0.986"/>
    <Spinner
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/SpinnerCoisasBeneficios" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeBeneficios" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView120" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.115"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:text="Por Tipo : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView123" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView120"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/SpinnerCoisasBeneficios"
            android:textColor="#161313"/>
    <TextView
            android:text="Ou por idade :"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/textView124"
            android:textColor="#161313" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView120"
            android:layout_marginBottom="444dp"/>
    <ImageView
            android:layout_width="129dp"
            android:layout_height="127dp" app:srcCompat="@drawable/beithbrasil"
            android:id="@+id/imageView32" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView124" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeBeneficios"
            app:layout_constraintVertical_bias="0.963"/>
    <Spinner
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/SpinnerDaIdade" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView32" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView124" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.071"
            android:layout_marginStart="8dp"/>
    <Button
            android:text="Pesquisa por idade"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button8" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/imageView32"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/SpinnerDaIdade" app:layout_constraintVertical_bias="0.11"/>
    <Button
            android:text="Pesquisa por Tipo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnPesquisaTipo" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/SpinnerCoisasBeneficios"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/textView124"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.153"/>
    <Button
            android:text="Recem chegado"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnRecemChegado" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/imageView32"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button8"/>

</androidx.constraintlayout.widget.ConstraintLayout>