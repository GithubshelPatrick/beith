<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".VideosActivity">

    <ImageView
            android:src="@drawable/videos"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/imageView59" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:src="@drawable/placayoutube"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/imageView62" app:layout_constraintStart_toEndOf="@+id/imageView59"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="Videos"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView119" app:layout_constraintEnd_toStartOf="@+id/imageView62"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/imageView59"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.029"
            android:textAlignment="center" android:textColor="#161313" android:textSize="30sp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="Canais"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnCanais" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView119" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.131"/>
    <Button
            android:text="Nossos videos"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/button13" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnCanais"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.06"/>
    <Button
            android:text="Curiosidades"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnCuriosidades" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button13"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.081"/>
    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            android:id="@+id/BtnVoltarDeVideos" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnCuriosidades"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="1.0"/>
    <ImageView
            android:layout_width="110dp"
            android:layout_height="110dp" app:srcCompat="@drawable/beithbrasil"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/imageView71" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeVideos" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnCuriosidades" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.937"/>
</androidx.constraintlayout.widget.ConstraintLayout>