<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FotoParaCadastrarActivity">

    <TextView
            android:text="Foto Para seu Cadastro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView100" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.062"
            android:textAlignment="center" android:textColor="#161313" android:textSize="30sp"/>
    <ImageView
            android:layout_width="130dp"
            android:layout_height="130dp" app:srcCompat="@drawable/foto"
            android:id="@+id/BtnAcessarGaleriaCad" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView100" app:layout_constraintVertical_bias="0.071"
            app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="130dp"
            android:layout_height="130dp" app:srcCompat="@drawable/camerazinha"
            android:id="@+id/CameraDoCadastro" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/BtnAcessarGaleriaCad" app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintVertical_bias="0.057"/>
    <TextView
            android:text="Galeria"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView102" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnAcessarGaleriaCad" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView100" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="1.0" android:textAlignment="center"/>
    <TextView
            android:text="Camera"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView108" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnAcessarGaleriaCad"
            app:layout_constraintBottom_toTopOf="@+id/CameraDoCadastro" android:layout_marginStart="8dp"
            app:layout_constraintVertical_bias="1.0" android:textAlignment="center"/>
    <ImageView
            android:layout_width="409dp"
            android:layout_height="155dp" app:srcCompat="@drawable/fotodois"
            android:id="@+id/MinhaFot"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/CameraDoCadastro"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnGostei"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="Gostei"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"
            android:id="@+id/BtnGostei" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"/>
    <Button
            android:text="Refazer"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:id="@+id/button24" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/MinhaFot"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintVertical_bias="0.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>