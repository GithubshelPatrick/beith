<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DicasActivity">

    <TextView
            android:text="Dicas "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView114"
            android:textSize="24sp" android:textColor="#161313"
            android:layout_marginTop="27dp" android:layout_marginBottom="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/BtnTurImg"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Turismo"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnDicasTurismo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/BtnTurImg"/>
    <Button
            android:text="Promoções"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnDicasPromocoes"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/BtnPromoImg" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"/>
    <Button
            android:text="Eventos "
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnDicasEvento"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/BtnTurImg"
            android:layout_marginBottom="273dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDicas"/>
    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnVoltarDeDicas"
            app:layout_constraintTop_toBottomOf="@+id/BtnDicasEvento" android:layout_marginBottom="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="100dp" app:srcCompat="@drawable/beithbrasil"
            android:id="@+id/imageView31"
            android:layout_marginBottom="72dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Gastronomia "
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnDicasGastronomia"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="160dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDicas"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="65dp" app:srcCompat="@drawable/tur"
            android:id="@+id/BtnTurImg" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnDicasEvento"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView114"
            android:layout_marginBottom="214dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="65dp" app:srcCompat="@drawable/promocao"
            android:id="@+id/BtnPromoImg"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnDicasPromocoes"
            app:layout_constraintTop_toBottomOf="@+id/BtnDicasTurismo"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="65dp" app:srcCompat="@drawable/evento"
            android:id="@+id/BtnEvenImg" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnDicasPromocoes"
            app:layout_constraintBottom_toTopOf="@+id/BtnDicasEvento"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="65dp" app:srcCompat="@drawable/gastronomia"
            android:id="@+id/BtnGastroImg"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnDicasGastronomia"
            app:layout_constraintTop_toBottomOf="@+id/BtnDicasEvento"/>

</androidx.constraintlayout.widget.ConstraintLayout>