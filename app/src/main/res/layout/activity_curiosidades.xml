<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CuriosidadesActivity">

    <TextView
            android:text="Videos legais"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView167" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.038" android:textColor="#161313" android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="92dp" app:srcCompat="@drawable/joia"
            android:id="@+id/imageView77" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/textView167"
            android:layout_marginEnd="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="8dp" android:id="@+id/BtnVoltarDeCuriosidades" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView167" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="1.0"/>
    <ImageView
            android:layout_width="62dp"
            android:layout_height="42dp" app:srcCompat="@drawable/um"
            android:id="@+id/imageView78" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeCuriosidades"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/imageView77"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.034"/>
    <TextView
            android:text="Brasileiros em Israel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView185" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView78" android:layout_marginStart="4dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeCuriosidades" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView167" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.089"/>
    <TextView
            android:text="https://www.youtube.com/watch?v=PtacvRYhlXc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView199" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView78" android:layout_marginStart="4dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeCuriosidades"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView185"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"
            android:textColor="#0000f2" android:textSize="12sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>