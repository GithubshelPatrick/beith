<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DuvidasActivity">

    <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp" app:srcCompat="@drawable/duvida"
            android:id="@+id/imageView17" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.025" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="Duvidas ?!?!?"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView49" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView17"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.057"
            android:textAlignment="center" android:textColor="#161313" android:textSize="30sp"/>
    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"
            android:id="@+id/BtnVoltarDeDuvidas" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/automovel"
            android:id="@+id/imageView25"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.024" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas" app:layout_constraintVertical_bias="0.068"/>
    <Button
            android:text="Autos e habilitação"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:id="@+id/BtnDuvidasAutos" app:layout_constraintStart_toEndOf="@+id/imageView25"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView49"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.069"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/casa"
            android:id="@+id/imageView18" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/imageView25"
            app:layout_constraintHorizontal_bias="0.023" app:layout_constraintVertical_bias="0.057"/>
    <Button
            android:text="Aluguel e compra de imoveis "
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:id="@+id/button2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView18"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnDuvidasAutos"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.055"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/documentos"
            android:id="@+id/imageView20"
            app:layout_constraintVertical_bias="0.077" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView18" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.023"/>
    <Button
            android:text="documentos e cotas"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:id="@+id/button3" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView20"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button2"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.077"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/escola"
            android:id="@+id/imageView23" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView20" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.023"
            app:layout_constraintVertical_bias="0.055"/>
    <Button
            android:text="Escola, cursos, Faculdade e revalidações"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:id="@+id/button4" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView23"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button3"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.059"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/interrogacao"
            android:id="@+id/imageView24"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/imageView23"
            app:layout_constraintHorizontal_bias="0.023" app:layout_constraintVertical_bias="0.7"/>
    <Button
            android:text="Mande sua pergunda"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:id="@+id/BtnMandeSuaPerguninha" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView24"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDuvidas"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button4"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.7"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/imposto" android:id="@+id/imageView27"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView24" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView23" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.023"/>
    <Button
            android:text="impostos e aposentadoria"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:id="@+id/button6" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/imageView27"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button4"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/BtnMandeSuaPerguninha"/>
</androidx.constraintlayout.widget.ConstraintLayout>